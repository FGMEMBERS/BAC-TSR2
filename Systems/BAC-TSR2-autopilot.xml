      <prop>/autopilot/internal/target-climb-rate-fps-agl</prop>
    </output>
    <config>
      <Ts>0.02</Ts>
      <Kp>0.16</Kp>
      <beta>1.0</beta>
      <alpha>0.1</alpha>
      <gamma>0.0</gamma>
      <Ti>20.0</Ti>
      <Td>0.1</Td>
      <u_min>-100.0</u_min>
      <u_max>300.0</u_max>
    </config>
  </pid-controller>

<!-- Stage #2 drives the elevator-trim to achieve the desired climb rate. -->
  <pid-controller>
    <name>AGL Hold Stage 2</name>
    <debug>false</debug>
    <enable>
      <prop>/autopilot/locks/altitude</prop>
